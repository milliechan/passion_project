<!DOCTYPE html>
<html>
<head>
  <title>Dashboard</title>
</head>
<body>
  <h1>Dashboard</h1>
  <div id="search">
  <form action="/search" method="GET"> 
    <input id="query" type="text" name="query" placeholder="Search for a video">
    <button type="submit" id="submit">Search</button>
  </form>
  </div>
  <div id="search_results">
    <h2>Search Results</h4>
  </div>
  <div id="queue">
    <h2>Next Singer</h4>
      <% if @queue %> 
        <% @queue.each do |item| %> 
          <%= item.user_id %>
          <%= item.title %>
          <%= item.url %> 
          <%= item.completed %> 
        <% end %>
      <% end %>
  </div>
</body>
</html>